    <!-- OpenMailFrame-style preview for logged mail -->
    <Frame name="EQOL_MailPreviewFrame" toplevel="true" hidden="true" parent="UIParent" enableMouse="true"
        movable="true" clampedToScreen="true" inherits="ButtonFrameTemplate">
        <Layers>
            <Layer level="OVERLAY" textureSubLevel="-1">
                <Texture parentKey="$parentIcon" file="Interface\MailFrame\Mail-Icon">
                    <Size x="60" y="60"/>
                    <Anchors>
                        <Anchor point="TOPLEFT" x="-6" y="7"/>
                    </Anchors>
                </Texture>
            </Layer>
            <Layer level="ARTWORK">
                <FontString parentKey="SenderLabel" inherits="GameFontHighlight" text="FROM">
                    <Size x="0" y="16"/>
                    <Anchors>
                        <Anchor point="TOPRIGHT" relativePoint="TOPLEFT" x="105" y="-33"/>
                    </Anchors>
                </FontString>
                <FontString parentKey="SubjectLabel" inherits="GameFontHighlight" text="MAIL_SUBJECT_LABEL">
                    <Size x="0" y="16"/>
                    <Anchors>
                        <Anchor point="TOPRIGHT" relativePoint="TOPLEFT" x="105" y="-55"/>
                    </Anchors>
                </FontString>
                <FontString parentKey="SubjectText" inherits="GameFontNormalSmall" justifyH="LEFT" justifyV="TOP">
                    <Size x="225" y="0"/>
                </FontString>
                <Texture parentKey="HorizontalBarLeft" file="Interface\ClassTrainerFrame\UI-ClassTrainer-HorizontalBar">
                    <Size x="256" y="16"/>
                    <Anchors>
                        <Anchor point="TOPLEFT" x="2" y="-390"/>
                    </Anchors>
                    <TexCoords left="0" right="1.0" top="0" bottom="0.25"/>
                </Texture>
                <Texture parentKey="HorizontalBarRight" file="Interface\ClassTrainerFrame\UI-ClassTrainer-HorizontalBar">
                    <Size x="75" y="16"/>
                    <TexCoords left="0" right="0.29296875" top="0.25" bottom="0.5"/>
                </Texture>
            </Layer>
        </Layers>
        <Frames>
            <Frame parentKey="Sender">
                <Layers>
                    <Layer level="ARTWORK">
                        <FontString parentKey="Name" inherits="GameFontNormal" justifyH="LEFT">
                            <Anchors>
                                <Anchor point="LEFT"/>
                                <Anchor point="RIGHT"/>
                            </Anchors>
                        </FontString>
                    </Layer>
                </Layers>
            </Frame>
            <ScrollFrame parentKey="ScrollFrame" inherits="ScrollFrameTemplate">
                <KeyValues>
                    <KeyValue key="scrollBarX" value="10" type="number"/>
                    <KeyValue key="scrollBarTopY" value="-3" type="number"/>
                    <KeyValue key="scrollBarBottomY" value="5" type="number"/>
                </KeyValues>
                <Size x="296" y="257"/>
                <Anchors>
                    <Anchor point="TOPLEFT" x="8" y="-84"/>
                </Anchors>
                <Layers>
                    <Layer level="BACKGROUND">
                        <Texture parentKey="StationeryBackgroundLeft">
                            <Size x="252" y="256"/>
                            <Anchors>
                                <Anchor point="TOPLEFT"/>
                            </Anchors>
                        </Texture>
                        <Texture parentKey="StationeryBackgroundRight">
                            <Size x="64" y="256"/>
                        </Texture>
                    </Layer>
                </Layers>
                <ScrollChild>
                    <Frame parentKey="ScrollChildFrame">
                        <Size x="296" y="256"/>
                        <Anchors>
                            <Anchor point="TOPLEFT" x="0" y="0"/>
                        </Anchors>
                        <Frames>
                            <SimpleHTML parentKey="BodyText" inherits="InlineHyperlinkFrameTemplate">
                                <Size x="276" y="1"/>
                                <Anchors>
                                    <Anchor point="TOPLEFT" x="10" y="-10"/>
                                </Anchors>
                                <FontString inherits="MailTextFontNormal" nonspacewrap="true" justifyH="LEFT"/>
                            </SimpleHTML>
                        </Frames>
                    </Frame>
                </ScrollChild>
            </ScrollFrame>
            <Frame parentKey="AttachmentsArea">
                <Size x="296" y="72"/>
                <Anchors>
                    <Anchor point="BOTTOMLEFT" x="18" y="12"/>
                </Anchors>
                <Layers>
                    <Layer level="ARTWORK">
                        <FontString parentKey="AttachmentsLabel" inherits="GameFontHighlightSmall" text="TAKE_ATTACHMENTS">
                        </FontString>
                    </Layer>
                </Layers>
            </Frame>
            <Button parentKey="CloseButtonBottom" inherits="UIPanelButtonTemplate" text="CLOSE">
                <Size x="80" y="22"/>
                <Anchors>
                    <Anchor point="BOTTOMRIGHT" relativePoint="BOTTOMRIGHT" x="-6" y="4"/>
                </Anchors>
            </Button>
        </Frames>
    </Frame>

